{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint ./src/ --ext=js,vue",
    "lint:fix": "eslint ./src --fix --ext=js,vue",
    "lint:safe": "eslint ./src --ext=js,vue ;exit 0",
    "build": "webpack --colors --progress --hide-modules --config webpack.config.js",
    "watch": "webpack --colors --progress --hide-modules --watch --config webpack.config.js",
    "test": "npm run lint",
    "test:safe": "npm run lint; exit 0"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Potato4d",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "electron": "^1.4.1",
    "eslint": "^3.8.1",
    "eslint-plugin-vue": "^0.1.1",
    "gulp": "^3.9.1",
    "gulp-s3-upload": "^1.6.1",
    "svg-url-loader": "^1.1.0",
    "vue": "^2.0.3",
    "vue-hot-reload-api": "^2.0.6",
    "vue-loader": "^9.7.0",
    "vue-router": "^2.0.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
